{
    "comments": {
        // symbol used for single line comment. Remove this entry if your language does not support line comments
        "lineComment": "//"
    },
    // symbols used as brackets
    "brackets": [
        ["beginFunc", "endFunc"],
        ["pushCtx", "popCtx"],
        ["pushRootCtx", "popCtx"],
        ["pushStack", "popStack"]
    ],
    // symbols that are auto closed when typing
    "autoClosingPairs": [
        ["beginFunc", "endFunc"],
        ["pushCtx", "popCtx"],
        ["pushRootCtx", "popCtx"],
        ["pushStack", "popStack"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // symbols that can be used to surround a selection
    "surroundingPairs": [
        ["beginFunc", "endFunc"],
        ["pushCtx", "popCtx"],
        ["pushRootCtx", "popCtx"],
        ["pushStack", "popStack"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s+(beginFunc|pushCtx|pushRootCtx|popCtx)\\s$",
        "decreaseIndentPattern": "^\\s+(endFunc|popCtx|popStack)\\s$"
    }
}